# Navs
'Nav - Bootstrap 4':
	'prefix': 'nav'
	'body': """
		<ul class="nav">
			<li class="nav-item">
				<a class="nav-link" href="${1:#}">${2:Link}</a>
			</li>
			<li class="nav-item">
				<a class="nav-link" href="${1:#}">${2:Link}</a>
