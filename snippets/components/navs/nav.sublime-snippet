<snippet>
	<content><![CDATA[
		<ul class="nav">
			<li class="nav-item">
				<a class="nav-link" href="${1:#}">${2:Link}</a>
			</li>
			<li class="nav-item">
				<a class="nav-link" href="${1:#}">${2:Link}</a>
			</li>
			<li class="nav-item">
				<a class="nav-link" href="${1:#}">${2:Another link}</a>
			</li>
			<li class="nav-item">
				<a class="nav-link disabled" href="${1:#}">${3:Disabled}</a>
			</li>
		</ul>
]]></content>
	<tabTrigger>nav</tabTrigger>
</snippet>


